#include<stdio.h>
# include<string.h>
void main()
{
   char str[60];
   int i,j;
    printf("enter any words");
   gets(str); 
  int   len=strlen(str)-1;
    for( i=0,j=len;i<=j;i++,j--)
    
       { 
          if(str[i]!=str[j])
         { 
           printf("not palindrome");
         return; 
         }
      }    printf("palindrome");          
    }   
