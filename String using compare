#include<stdio.h>
#include<string.h>
char string1[20]; char string2[20];
int main()
{
    printf("enter your password");
    fgets(string1,sizeof(string2),stdin);
    printf("reenter the password ");
    fgets(string2,sizeof(string2),stdin);
    if ( strcmp(string1, string2)==0)
    {
        printf("your password is matched");
    }
  else
  {
      printf("your password is not matched");
  }
}
